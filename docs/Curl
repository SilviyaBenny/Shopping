
PRODUCT

curl -X POST "http://localhost:8080/product" -H "accept: */*" -H "Content-Type: application/json" -d "{\"name\":\"Book\",\"quantity\":10,\"price\":100,\"sku\":\"01A\",\"departmentId\":20}"
curl -X GET "http://localhost:8080/product" -H "accept: */*"
curl -X GET "http://localhost:8080/product/1" -H "accept: */*"
curl -X PUT "http://localhost:8080/product/5" -H "accept: */*" -H "Content-Type: application/json" -d "{\"name\":\"PencilBox\",\"quantity\":10,\"price\":100,\"sku\":\"01A\",\"departmentId\":20}"
curl -X DELETE "http://localhost:8080/product/26" -H "accept: */*"

DEPARTMENT

curl -X POST "http://localhost:8080/department" -H "accept: */*" -H "Content-Type: application/json" -d "{\"departmentName\":\"Garden\",\"description\":\"Gardening\"}"
curl -X GET "http://localhost:8080/department" -H "accept: */*"
curl -X GET "http://localhost:8080/department/1" -H "accept: */*"
curl -X PUT "http://localhost:8080/department/5" -H "accept: */*" -H "Content-Type: application/json" -d "{\"departmentName\":\"Garden\",\"description\":\"Garden Supplies\"}"
curl -X DELETE "http://localhost:8080/department/31" -H "accept: */*"

